TARGET_CFLAGS += -fno-strict-aliasing
TARGET_CXXFLAGS += -fno-strict-aliasing
PY_VER := 2.7
PY_LD_VER := 2.7
PY_INCDIRS := $(PYTHON_DIR)/include/python2.7
PY_LIBDIRS := $(PYTHON_DIR)/lib
#ifeq '$(PCDS_BUILD)' 'YES'
PY_INCDIRS += $(NUMPY_DIR)/lib/python2.7/site-packages/numpy-$(NUMPY_FULL_VERSION)-py2.7-linux-x86_64.egg/numpy/core/include 
#endif
HAVE_NUMPY := YES
PY_OK := YES
